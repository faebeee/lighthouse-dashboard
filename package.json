{
    "name": "dreihouse-dashboard",
    "version": "4.1.1",
    "description": "A Vue.js project",
    "main": "src/main.js",
    "private": true,
    "scripts": {
        "dev": "webpack-dev-server --inline --progress --config build/webpack.dev.conf.js",
        "start": "node -r esm ./src/server2/index.js",
        "lint": "eslint --ext .js,.vue src test/unit",
        "build": "npm run build-server && node build/build.js",
        "build-server": "node_modules/.bin/tsc -p src/server",
        "server": "NODE_ENV=production node dist/server/index.js",
        "server-dev": "NODE_ENV=development node_modules/.bin/nodemon",
        "deploy": "git push deis master"
    },
    "types": "./src/types/index.d.ts",
    "dependencies": {
        "@hapi/hapi": "^18.4.0",
        "@hapi/inert": "^5.2.2",
        "@hapi/joi": "^16.1.8",
        "@hapi/vision": "^5.5.4",
        "axios": "^0.19.0",
        "chromium": "^2.1.1",
        "dotenv": "^8.2.0",
        "esm": "^3.2.25",
        "hapi-error-page": "^1.0.0",
        "hapi-swagger": "^11.1.0",
        "laabr": "^5.1.5",
        "lodash.curry": "^4.1.1",
        "mongodb": "^3.4.0",
        "promisify-node": "^0.5.0",
        "pwmetrics": "^4.2.2"
    },
    "devDependencies": {
        "@types/hapi": "^18.0.3",
        "nodemon": "^2.0.2"
    },
    "engines": {
        "node": ">= 8.0.0",
        "npm": ">= 5.0.0"
    },
    "browserslist": [
        "> 1%",
        "last 2 versions",
        "not ie <= 8"
    ]
}
