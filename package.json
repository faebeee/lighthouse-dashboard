{
    "name": "dreihouse-dashboard",
    "version": "5.9.21",
    "private": true,
    "description": "App to keep track of your websites by showing their lighthouse results",
    "author": {
        "email": "fabio.gianini@dreipol.ch",
        "name": "fabs",
        "url": "https://www.fabs.io"
    },
    "repository": {
        "url": "https://github.com/dreipol/lighthouse-dashboard",
        "type": "git"
    },
    "scripts": {
        "serve": "vue-cli-service serve",
        "build": "vue-cli-service build --mode production",
        "lint": "npm run lint-frontend && npm run lint-backend",
        "lint-frontend": "eslint -c .eslintrc ./assets/src",
        "lint-backend": "eslint -c .eslintrc ./src",
        "dev": "NODE_ENV=development node_modules/.bin/nodemon",
        "start": "node -r esm index.js",
        "audit": "node -r esm src/cli/index.js -a --queue",
        "worker": "node -r esm src/worker/index.js",
        "test": "npm run lint && mocha src/**/*.spec.js -r esm"
    },
    "main": "assets/src/app/main.js",
    "dependencies": {
        "@hapi/boom": "^8.0.1",
        "@hapi/hapi": "^19.1.1",
        "@hapi/inert": "^6.0.1",
        "@hapi/joi": "^17.1.1",
        "@hapi/vision": "^6.0.0",
        "@sentry/node": "^5.15.4",
        "@types/amqplib": "^0.5.13",
        "amqplib": "^0.5.5",
        "apexcharts": "^3.18.1",
        "axios": "^0.19.2",
        "blipp": "^4.0.1",
        "chai": "^4.2.0",
        "chrome-launcher": "^0.13.1",
        "chrome-paths": "^1.0.1",
        "commander": "^4.1.1",
        "core-js": "^3.6.5",
        "date-fns": "^2.12.0",
        "dotenv": "^8.2.0",
        "es6-promise": "^4.2.8",
        "esm": "^3.2.25",
        "fancy-log": "^1.3.3",
        "glob": "^7.1.6",
        "hapi-auth-jwt2": "^8.8.1",
        "hapi-dev-errors": "^3.3.0",
        "hapi-swagger": "^13.0.0",
        "jsonwebtoken": "^8.5.1",
        "laabr": "^5.1.6",
        "lighthouse": "^5.6.0",
        "lodash.chunk": "^4.2.0",
        "lodash.curry": "^4.1.1",
        "lodash.uniq": "^4.5.0",
        "mocha": "^7.1.1",
        "mongodb": "^3.5.5",
        "puppeteer": "^2.1.1",
        "uuid": "^3.4.0",
        "vue": "^2.6.10",
        "vue-router": "^3.1.6",
        "vuetify": "^2.2.21",
        "vuex": "^3.1.3",
        "winston": "^3.2.1"
    },
    "devDependencies": {
        "@babel/plugin-syntax-dynamic-import": "^7.8.3",
        "@dreipol/eslint-config": "^6.0.5",
        "@types/hapi": "^18.0.3",
        "@vue/cli-plugin-babel": "^4.3.1",
        "@vue/cli-plugin-eslint": "^4.3.1",
        "@vue/cli-service": "^4.3.1",
        "babel-eslint": "^10.1.0",
        "babel-plugin-transform-runtime": "^6.23.0",
        "babel-plugin-transform-vue-jsx": "^3.7.0",
        "babel-preset-env": "^1.7.0",
        "babel-preset-stage-2": "^6.24.1",
        "eslint": "^5.16.0",
        "eslint-plugin-vue": "^5.0.0",
        "node-sass": "^4.13.1",
        "nodemon": "^2.0.3",
        "sass": "^1.26.3",
        "sass-loader": "^8.0.2",
        "vue-cli-plugin-vuetify": "^2.0.5",
        "vue-template-compiler": "^2.6.10",
        "vuetify-loader": "^1.3.0"
    },
    "eslintConfig": {
        "root": true,
        "env": {
            "node": true
        },
        "extends": [
            "plugin:vue/essential",
            "eslint:recommended"
        ],
        "rules": {},
        "parserOptions": {
            "parser": "babel-eslint"
        }
    },
    "browserslist": [
        "> 1%",
        "last 2 versions",
        "not ie <= 8"
    ],
    "engines": {
        "node": ">= 12.0",
        "npm": ">= 6.0.0"
    },
    "types": "./types/index.d.ts"
}
